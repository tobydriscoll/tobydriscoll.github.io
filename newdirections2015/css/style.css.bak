/** BASIC */

html {
    overflow-y: scroll
}

body {
	margin: 0px;
	padding: 0px;
	font-size: 14px;
	color: #000; 
	background-color: #FFF;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
}

a:link {
	color: #004784;
	background: inherit;
	text-decoration: none;
}

a:visited {
	color: #004784;
	background: inherit;
	text-decoration: none;
}

a:hover {
	color:#627684; 
	background-color: inherit;
	text-decoration: none;
}

h1 {
	font-family: Arial, Helvetica, sans-serif;
	font-weight: bold;
	text-transform: uppercase;
}

h2 {
	font-size: 14px;
	text-transform: uppercase;
}

h2.bigger {
	font-size: 18px;
	text-transform: uppercase;
    margin-top: 10px;
    margin-bottom: 8px;
}

h3 {
	margin: 0px;
	padding: 0px;
	font-size: 11px;
	text-transform: uppercase;
}

p, ol, ul {
	margin-top: 0px;
}

.hr1 {
	clear: both;
	margin: 2px 0px 2px 0px;
	border: 1px dashed #4d506d;
}

.hr1 hr {
	display: none;
}

table {
	margin-left: auto;
	margin-right: auto;
}

td {
	padding:0;
}

span.comment {
	color:green;
} 

/** WRAPPER */

#wrapper {
        width: 715px;
        margin: 0px auto;
        padding: 0px;
        padding-left: 32px;
        padding-right: 30px;
        color: #000;
        background: url('../images/bg_body.gif') repeat-y;
        background-color: #FFF;
        height: auto;
}

#headimg {
        width: 715px;
        margin: 0px auto;
        padding: 0px;
        padding-left: 32px;
        padding-right: 30px;
        color: #000;
        background-color: #FFF;
        height: 12px;
        background: url('../images/bg_head.gif') no-repeat;
}

#footimg {
        width: 715px;
        margin: 0px auto;
        padding: 0px;
        padding-left: 32px;
        padding-right: 30px;
        color: #000;
        background-color: #FFF;
        height: 12px;
        background: url('../images/bg_footer.gif') no-repeat;
}

/** HEADER */

#myheader {
	color: inherit;
	background-color: inherit;
	margin-bottom: 0px;
	padding: 0px;
	text-align: center;
	height: 68px;
	padding-top: 0px;
}

#myheader h1 {
	margin: 0px;
	padding: 0px;
	font-size: 30px;
}

#myheader h2 {
	margin: 0px;
	padding: 0px;
	font-size: 12px;
	text-transform: lowercase;
}

/** LOGO */

a.headerA { 
	text-decoration: none;
    text-transform: uppercase;
	letter-spacing: 0em;
	font-family: sans-serif;
	font-size: 18px;
	font-weight: normal;
	color:#002147;  
	background-color: inherit;
    float:right;
    margin: 0px;
    margin-top: 30px;
    margin-right:2px;
    height:18px;
}

a.headerB {
	text-decoration: none;
    text-transform: uppercase;
	letter-spacing: 0em;
	font-family: sans-serif;
	font-size: 18px;
    height:18px;
    line-height: 18px;
	font-weight: normal;
	color:#887B1B;
	background-color: inherit;
    float:right;
    margin: 0px;
    margin-right: 2px;
}

img.logoimg {
    width: 300px;
    border: 0px;
    float:left;
    padding: 2px;
}

#fbsharediv {
    float:right; 
    margin-left:0px; 
    margin-bottom:1px; 
    margin-top:1px; 
    padding-top:1px;
}

#tweetdiv {
    float:right; 
    margin-left:4px; 
    margin-right:2px; 
    margin-bottom:0px; 
    margin-top:0px; 
    width:55px; 
    overflow:hidden;
}

/** MAIN-MENU */

#main-menu {
	margin: 0px 0px 0px 0px;
	color: inherit;
	background: #4d506d /* The color of the top header bar */	top; 
	text-transform: uppercase;
	font-size: 11px;
	text-align: center;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
}

#main-menu ul {
	margin: 0px;
	padding: 7px 0px 7px 0px;
	list-style: none;
}

#main-menu li {
	display: inline;
}

#main-menu a {
	padding: 0px 8px 0px 15px;
	color: #FFF;
	background: transparent url(../images/bullets.gif) no-repeat left center;
	text-decoration: none;
	font-weight: bold;
}

#main-menu a:hover {
	color:#cbbb53;
/*	background: transparent url(../images/bullets_hover.gif) no-repeat left center; */
	background: transparent url(../images/bullets.gif) no-repeat left center;
}

/* Guide menu */

#guide-menu {
	margin: 0px 0px 0px 0px;
	color: inherit;
	background: #4d506d /* The color of the top header bar */	top; 
	font-size: 11px;
	text-align: center;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
    position: fixed;
	bottom: 0px;
    /*width: 14px;*/
    overflow:visible;
	text-transform: uppercase;
	width: 715px;
}


#guide-menu ul {
	margin: 0px;
	padding: 0px 0px 0px 0px;
	list-style: none;
}

#guide-menu li {
	display: inline;
}

#guide-menu a {
	padding: 0px 7px 0px 7px;
	color: #FFF;
	background: #4d506d /* The color of the top header bar */ top;
	text-decoration: none;
	font-weight: bold;
}

#guide-menu a:hover {
	color:#cbbb53;
	padding: 0px 7px 0px 7px;
	background: #4d506d /* The color of the top header bar */
}

a span.show {display:none;}
a:hover span.show {display:inline;}
a:hover span.noshow {display:none;}

/* Round corners */

b.rtop, b.rbottom { /* Setting the corner color, use different color for different layout */
    display:block;
    background: #FFF
} 
b.rtop b, b.rbottom b { /* Setting the strip size and background color */
    display:block;
    height: 1px; 
    overflow: hidden; 
    background: #4d506d;
}

b.rtop2, b.rbottom2 { /* Setting the corner color, use different color for different layout */
    display:block;
    background: #FFF
} 
b.rtop2 b, b.rbottom2 b { /* Setting the strip size and background color */
    display:block;
    height: 1px; 
    overflow: hidden; 
    background: #d9e3e5;
}

b.r1 {margin: 0 2px;}
b.r2 {margin: 0 1px;}
b.r3 {margin: 0 0px;}
b.rtop b.r4, b.rbottom b.r4 {
    margin: 0px 0px; 
    height: 1px;
}

